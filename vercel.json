{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/nav-items",
      "dest": "/server.js"
    },
    {
      "src": "/api/accommodations",
      "dest": "/server.js"
    },
    {
      "src": "/api/accommodations/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/api/meal-plans",
      "dest": "/server.js"
    },
    {
      "src": "/api/activities",
      "dest": "/server.js"
    },
    {
      "src": "/api/faqs",
      "dest": "/server.js"
    },
    {
      "src": "/api/gallery-images",
      "dest": "/server.js"
    },
    {
      "src": "/api/testimonials",
      "dest": "/server.js"
    },
    {
      "src": "/api/nearby-locations",
      "dest": "/server.js"
    },
    {
      "src": "/api/packages",
      "dest": "/server.js"
    },
    {
      "src": "/api/packages/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/api/all-data",
      "dest": "/server.js"
    },
    {
      "src": "/api/all-images",
      "dest": "/server.js"
    },
    {
      "src": "/api/bookings",
      "dest": "/server.js"
    },
    {
      "src": "/api/payments/payu",
      "dest": "/server.js"
    },
    {
      "src": "/api/health",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "server.js": {
      "maxDuration": 30
    }
  }
}
